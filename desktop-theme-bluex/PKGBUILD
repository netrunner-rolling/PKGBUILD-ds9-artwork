pkgname=desktop-theme-bluex
pkgver=$(date +%Y.%m)
pkgrel=1
pkgdesc="Bluex Plasma Theme"
arch=('any')
url='https://github.com/ds9-artwork/desktop-theme-bluex'
license=('GPL')
groups=('ds9-artwork')
makedepends=('git')
source=('git://github.com/ds9-artwork/desktop-theme-bluex')
md5sums=('SKIP')

package() {
  cd $srcdir/$pkgname
  cp -aR usr $pkgdir/
}
