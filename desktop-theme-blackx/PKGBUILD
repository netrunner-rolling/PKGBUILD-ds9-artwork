pkgname=desktop-theme-blackx
pkgver=$(date +%Y.%m)
pkgrel=1
pkgdesc="BlackX Plasma Theme"
arch=('any')
url='https://github.com/ds9-artwork/desktop-theme-blackx'
license=('GPL')
groups=('ds9-artwork')
makedepends=('git')
source=('git://github.com/ds9-artwork/desktop-theme-blackx')
md5sums=('SKIP')

package() {
  cd $srcdir/$pkgname
  cp -aR usr $pkgdir/
}
